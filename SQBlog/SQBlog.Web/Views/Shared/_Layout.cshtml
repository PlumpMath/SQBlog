<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width" />
        <title>@ViewBag.Title</title>
        <link href="~/Assets/css/bootstrap.css" rel="stylesheet" />
        <link href="~/Assets/css/main.css" rel="stylesheet" />
    </head>
    <body>
        <div class="navbar navbar-fixed-top navbar-inverse">
            <div class="navbar-inner">
                <div class="container">
                    <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse"><span
                            class="icon-bar"></span><span class="icon-bar"></span><span class="icon-bar"></span>
                    </a><a class="brand" style="color: white;" href="~/">Scott Qian Lab</a>
                    <ul class="nav">
                        <li><a href="http://www.scottqian.com">Blog</a></li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="navbar-placeholder" style="height: 60px;">
        </div>
        <div class="container">
            <div class="row">
                <div class="span3 div-container">
                    <ul class="nav nav-list">
                        <li class="nav-header">
                             <i class="icon-home"></i> DotNet 
                        </li>
                        <li>
                            <a href="~/chat">Web Chat</a>
                        </li>
                    </ul>
                </div>
                <div class="span9">
                    <div class="row-fluid div-container">
                        @RenderBody()
                    </div>
                </div>
            </div>
        </div>
        <script type="text/javascript" src="~/Assets/Scripts/jquery-1.8.3.min.js"></script>
        <script type="text/javascript" src="~/Assets/Scripts/bootstrap.js"></script>
        @RenderSection("scripts", required: false)
        <script type='text/javascript'>
            (function () {
                var c = document.createElement('script');
                c.type = 'text/javascript';
                c.async = true;
                c.src = ('https:' == document.location.protocol ? 'https://' : 'http://') + 'www.clicki.cn/boot/48061';
                var h = document.getElementsByTagName('script')[0];
                h.parentNode.insertBefore(c, h);
            })();
        </script>
    </body>
</html>
